            "content": "'use client';\n\nimport React from 'react';\nimport { PhoneIcon, MapPinIcon, EnvelopeIcon } from \"@heroicons/react/24/solid\";\nimport { motion } from 'framer-motion';\n\n// Animation variants\nconst containerVariants = {\n  hidden: { opacity: 0, y: 50 },\n  visible: {\n    opacity: 1,\n    y: 0,\n    transition: {\n      duration: 0.6,\n      staggerChildren: 0.2,\n    },\n  },\n};\n\nconst itemVariants = {\n  hidden: { opacity: 0, y: 20 },\n  visible: { opacity: 1, y: 0, transition: { duration: 0.5 } },\n};\n\nfunction ContactUsPage() {\n  return (\n    <>\n      {/* Hero Section */}\n      <section className=\"relative h-[500px] flex items-center justify-center text-white text-center overflow-hidden\">\n        {/* Background Image */}\n        <img\n          src=\"/pain.png\"\n          alt=\"Background\"\n          className=\"absolute inset-0 w-full h-full object-cover object-center opacity-80\"\n        />\n\n        {/* Overlay */}\n        <div className=\"absolute inset-0 bg-black bg-opacity-40\"></div>\n\n        {/* Notched Heading */}\n        <motion.div\n          variants={containerVariants}\n          initial=\"hidden\"\n          whileInView=\"visible\"\n          viewport={{ once: true }}\n          className=\"relative z-10 px-4\"\n        >\n          <div className=\"relative flex flex-col py-10 w-full bg-lime-400 border border-white border-solid rounded-[32px] overflow-visible\">\n            {/* Left notch */}\n            <div\n              className=\"absolute -left-8 top-1/2 w-[60px] h-[60px] bg-black rounded-full transform -translate-y-1/2 max-md:-left-4 max-md:w-[40px] max-md:h-[40px]\"\n            />\n            {/* Right notch */}\n            <div\n              className=\"absolute -right-8 top-1/2 w-[60px] h-[60px] bg-black rounded-full transform -translate-y-1/2 max-md:-right-4 max-md:w-[40px] max-md:h-[40px]\"\n            />\n            <h1 className=\"self-center text-4xl md:text-6xl font-extrabold uppercase text-black\">\n              Get In Touch With Us\n            </h1>\n          </div>\n        </motion.div>\n      </section>\n\n      {/* Contact Info Section */}\n      <section className=\"bg-black text-white py-20\">\n        <motion.div\n          variants={containerVariants}\n          initial=\"hidden\"\n          whileInView=\"visible\"\n          viewport={{ once: true }}\n          className=\"max-w-7xl mx-auto px-4\"\n        >\n          <div className=\"grid grid-cols-1 md:grid-cols-3 gap-12 justify-center items-center\">\n            {/* Location */}\n            <motion.div\n              variants={itemVariants}\n              className=\"flex items-center space-x-4 p-6 bg-gradient-to-b from-black to-[#001f1f] border border-lime-400 rounded-xl shadow-lg hover:shadow-lime-400/20 transition-shadow duration-300\"\n            >\n              <div className=\"bg-lime-400 p-3 rounded\">\n                <MapPinIcon className=\"h-10 w-10 text-black\" />\n              </div>\n              <div>\n                <h4 className=\"uppercase text-sm font-bold tracking-wide text-white\">Location</h4>\n                <p className=\"text-gray-200 text-xs\">MPS Building, Mullur Sarjapur Road</p>\n                <a\n                  href=\"https://www.google.com/maps?q=Site+No+40+%26+41,+1st+floor,+MPS+Building,+Ambedkar+Nagar,+Mullur+Sarjapur+Road,+Carmelram\"\n                  target=\"_blank\"\n                  rel=\"noopener noreferrer\"\n                  className=\"text-lime-400 text-xs underline\"\n                >\n                  View Location\n                </a>\n              </div>\n            </motion.div>\n\n            {/* Phone */}\n            <motion.div\n              variants={itemVariants}\n              className=\"flex items-center space-x-4 p-6 bg-gradient-to-b from-black to-[#001f1f] border border-lime-400 rounded-xl shadow-lg hover:shadow-lime-400/20 transition-shadow duration-300\"\n            >\n              <div className=\"bg-lime-400 p-3 rounded\">\n                <PhoneIcon className=\"h-10 w-10 text-black\" />\n              </div>\n              <div>\n                <h4 className=\"uppercase text-sm font-bold tracking-wide text-white\">Phone</h4>\n                <p className=\"text-gray-200\">9353555755, 9353555855</p>\n              </div>\n            </motion.div>\n\n            {/* Email */}\n            <motion.div\n              variants={itemVariants}\n              className=\"flex items-center space-x-4 p-6 bg-gradient-to-b from-black to-[#001f1f] border border-lime-400 rounded-xl shadow-lg hover:shadow-lime-400/20 transition-shadow duration-300\"\n            >\n              <div className=\"bg-lime-400 p-3 rounded\">\n                <EnvelopeIcon className=\"h-10 w-10 text-black\" />\n              </div>\n              <div>\n                <h4 className=\"uppercase text-sm font-bold tracking-wide text-white\">Email</h4>\n                <p className=\"text-gray-200\">fivesportshospital@gmail.com</p>\n              </div>\n            </motion.div>\n          </div>\n        </motion.div>\n      </section>\n\n      {/* Contact Message Section */}\n      <section className=\"bg-black text-white py-16 px-4\">\n        <motion.div\n          variants={containerVariants}\n          initial=\"hidden\"\n          whileInView=\"visible\"\n          viewport={{ once: true }}\n          className=\"max-w-7xl mx-auto flex flex-col md:flex-row items-center justify-between\"\n        >\n          {/* Left Side: Image */}\n          <motion.div\n            variants={itemVariants}\n            className=\"md:w-1/2 space-y-6 mb-8 md:mb-0 md:mr-8\"\n          >\n            <img\n              src=\"/3.png\"\n              alt=\"Two People in Sports Attire\"\n              className=\"w-full h-auto object-cover rounded-lg shadow-lg border border-lime-400\"\n            />\n          </motion.div>\n\n          {/* Right Side: Contact Message */}\n          <motion.div\n            variants={itemVariants}\n            className=\"md:w-1/2 bg-gradient-to-b from-black to-[#001f1f] p-8 rounded-lg shadow-xl border border-lime-400\"\n          >\n            <div className=\"space-y-4 mb-10\">\n              <h3 className=\"text-lime-400 uppercase font-bold tracking-wide text-sm\">\n                Contact Us\n              </h3>\n              <h2 className=\"text-4xl md:text-5xl font-extrabold text-white\">ASK US ANYTHING.</h2>\n              <p className=\"text-gray-200 leading-relaxed\">\n                We are here to assist you with your inquiries. Feel free to reach out!\n              </p>\n            </div>\n            <div className=\"space-y-4\">\n              <p className=\"text-gray-200\">\n                Please contact us using the following details:\n              </p>\n              <p className=\"text-gray-200\">\n                <span className=\"text-lime-400 font-bold\">Phone:</span> 9353555755, 9353555855\n              </p>\n              <p className=\"text-gray-200\">\n                <span className=\"text-lime-400 font-bold\">Email:</span> fivesportshospital@gmail.com\n              </p>\n              <p className=\"text-gray-200\">\n                Our team will respond to your inquiry as soon as possible.\n              </p>\n            </div>\n          </motion.div>\n        </motion.div>\n      </section>\n    </>\n  );\n}\n\nexport default ContactUsPage;"
        }
    ]
}